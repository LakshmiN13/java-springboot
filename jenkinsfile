pipeline{
    agent any

    stages{
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'mvn clean package'
            }
        }
        stage('Test'){
            steps {
                echo 'Testing..'
                sh 'mvn test'
            }
        }
        stage('user acceptance testing'){
            steps{
                echo 'user acceptance testing'
        
            }
        }
        stage('Deploy'){
            steps{
                echo 'Deploying .....'
            }
        }
    }
}



    
